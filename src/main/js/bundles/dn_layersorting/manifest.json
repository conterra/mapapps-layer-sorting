{
    "name": "dn_layersorting",
    "version": "1.0.0-SNAPSHOT",
    "dependencies": {
        "esri": "^4.15.0",
        "apprt-core": "^4.15.0",
        "notifier": "^4.15.0"
    },
    "main": "",
    "i18n": ["bundle"],
    "startLevel": 10,
    "components": [
        {
            "name": "Config",
            "impl": "BundleSortingModel",
            "provides": "dn_layersorting.BundleSortingModel",
            "propertiesConstructor": true,
            "properties": {
                "applicationDelay": 10000,
                "showRemainingBundleContents": {},
                "bundleOrderConfiguration": []
            }
        },
        {
            "name": "BundleSortingController",
            "references": [
                {
                    "name": "_model",
                    "providing": "dn_layersorting.BundleSortingModel"
                },
                {
                    "name": "_mapWidgetModel",
                    "providing": "map-widget.MapWidgetModel"
                },
                {
                    "name": "_logService",
                    "providing": "ct.framework.api.LogService"
                }
            ]
        },
         {
            "name": "DomainBundlePreprocessor",
            "provides": "domains-system.ConfigFragmentInterpreter",
            "properties": {
                "priority": 1
            },
            "references": [
                // {
                //     "name": "_basemapsModel",
                //     "providing": "map-basemaps-api.BasemapsModel",
                //     "cardinality": "0..1"
                // }
            ]
        }
    ]
}
